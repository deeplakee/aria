class Animal{
	init(name){
		this.name = name;
	}
	hello(){
		println("hello");
	}
}

class Dog : Animal{
	init(name){
		super.init(name);
	}
	animal_hello(){
	    super.hello();
	}
	hello(){
		println("WangWang");
	}
}

var dog = Dog("Bob");
println(dog.name);
dog.hello();
dog.animal_hello();
dog.age = 3;
println(dog.age);